{
	"expo": {
		"name": "modak-challenge-fran",
		"slug": "modak-challenge-fran",
		"scheme": "modak-challenge-fran",
		"version": "0.0.1",
		"author": "Francisco Miguel Peralta",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"]
	}
}
